<% 
  const getPageTitle = (pageMeta) => {
    var defaultPageTitle = "Pepo";

    if ( !pageMeta || !pageMeta.title ) {
      return defaultPageTitle;
    }
    var title = pageMeta.title;
    if ( pageMeta.page ) {
      title = title + " | " + pageMeta.page;
    }
    return title;
  }
%>

<title><%= getPageTitle(pageMeta) %></title>
<meta charset="utf-8">
<meta name="active-menu" content="">
<meta name="viewport" content="width=device-width, initial-scale=1">
