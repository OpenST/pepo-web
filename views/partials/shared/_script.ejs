<% if (pageMeta && pageMeta.assets ) {
   let files = pageMeta.assets.js;
   if ( files && files.length ) {
      let len = files.length, cnt = 0;
      for(cnt=0; cnt<len; cnt++) {
          let assetBundle = files[cnt];
          let linkTags = js(assetBundle);
          %><%- linkTags %><%
      }   
   } 
} %>