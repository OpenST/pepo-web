<div class="col-md-6 col-lg-6 innerLeft py-3 h-100" style="overflow-y: scroll!important;">
    <div class="media align-items-center">
        <img class="mr-2 profileImage img-fluid" src="<%=formattedEntityData.original_profile_image_url%>" alt="">
        <div class="media-body">
            <h6 class="m-0 user-name"><%= formattedEntityData.profile_name %></h6>
            <span class="user-handle">@<%= formattedEntityData.profile_username %></span>
        </div>
    </div>
    <% if (formattedEntityData.description_text) { %>
        <p class="mt-3 mb-0 desc" style="font-size: 15px"><%- formattedEntityData.description_text %></p>
    <% } %>
    <% if (formattedEntityData.description_link) { %>
        <a href="<%= formattedEntityData.description_link %>" class="mt-2 desc-link" rel="noopener noreferrer nofollow" target="_blank"><%- formattedEntityData.description_display_link %></a>
    <% } %>
    <ul class="mt-3 list-unstyled channel-list web">
        <% for(var i=0; i < formattedEntityData.channels_list.length; i++) { %>
            <li class="downloadApp"><a href="javascript:void(0)"><%= formattedEntityData.channels_list[i] %></a></li>
        <% } %>
    </ul>
</div>
