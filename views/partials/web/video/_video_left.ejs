<div class="col-md-6 col-lg-6 innerLeft py-3 h-100" style="overflow-y: scroll!important;overflow-x: hidden">
    <div class="media align-items-center">
        <div class="position-relative" style="font-size: 0">
            <span class="user-empty-profile-icon"></span>
            <img class="profileImage img-fluid" src="<%=formattedEntityData.original_profile_image_url%>" alt="">
        </div>
        <div class="media-body profile-media-body" style="width: calc(100% - 50px);">
            <div class="d-flex align-items-center">
                <h6 class="m-0 user-name"><%= formattedEntityData.profile_name %></h6>
                <div class="ml-2 d-flex align-items-center">
                    <svg width="12" height="12">
                        <switch>
                            <use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#video-web-cta-icon"></use>
                        </switch>
                    </svg>
                    <span class="timerCta web"><%= formattedEntityData.display_cts %></span>
                </div>
            </div>
            <span class="user-handle">@<%= formattedEntityData.profile_username %></span>
        </div>
    </div>
    <% if (formattedEntityData.description_text) { %>
        <p class="mb-0 desc"><%- formattedEntityData.description_text %></p>
    <% } %>
    <% if (formattedEntityData.description_link) { %>
        <a href="<%= formattedEntityData.description_link %>" class="desc-link" rel="noopener noreferrer nofollow" target="_blank"><%- formattedEntityData.description_display_link %></a>
    <% } %>
    <ul class="mt-3 list-unstyled channel-list web">
        <% for(var i=0; i < formattedEntityData.channels_list.length; i++) { %>
            <li class="downloadApp"><a href="javascript:void(0)"><%= formattedEntityData.channels_list[i] %></a></li>
        <% } %>
    </ul>
</div>
