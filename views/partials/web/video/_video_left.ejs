<div class="col-md-6 col-lg-6 innerLeft py-3 h-100" style="overflow-y: scroll!important;">
    <div class="media align-items-center">
        <img class="mr-2 profileImage img-fluid" src="<%=original_profile_image_url%>" alt="">
        <div class="media-body">
            <h6 class="m-0 user-name"><%= profile_name %></h6>
            <span class="user-handle">@<%= profile_username %></span>
        </div>
    </div>
    <% if (description_text) { %>
        <p class="mt-3 mb-0 desc" style="font-size: 15px"><%= description_text %></p>
    <% } %>
    <% if (description_link) { %>
        <a href="<%= description_link %>" class="mt-2 d-inline-block desc-link" rel="noopener noreferrer nofollow" target="_blank"><%= description_link %></a>
    <% } %>
    <ul class="mt-3 list-unstyled channel-list web">
        <% for(var i=0; i < channels_list.length; i++) { %>
            <li class="downloadApp"><a href="javascript:void(0)"><%= channels_list[i] %></a></li>
        <% } %>
    </ul>
</div>
